#define TAM_TITULO 128
#define TAM_AUTOR 50

#ifndef LIBRO_H_
#define LIBRO_H_

typedef struct{

	int id;
	char autor[TAM_AUTOR];
	char titulo[TAM_TITULO];
	float precio;


}eLibros;

eLibros* new_Libro();
eLibros* new_LibroParametros(int id, char* nombreStr, int horasTrabajadas, int sueldo);

#endif /* LIBRO_H_ */
